<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.add.ae5">
	<class name="Libro" table="libros">
		<id name="id" column="id">
			<generator class="increment"/>
		</id>
		<property name="titulo" type="string" column="titulo"/>
		<property name="autor" type="string" column="autor"/>
		<property name="anyoNac" type="string" column="anyo_nac"/>
		<property name="anyoPub" type="string" column="anyo_pub"/>
		<property name="editorial" type="string" column="editorial"/>
		<property name="pags" type="string" column="pags"/>
	</class>
</hibernate-mapping>
